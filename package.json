{
  "name": "express-babel-starter",
  "version": "1.0.0",
  "description": "Express babel starter kit for beginer",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec babel-node --presets env",
    "build": "babel src -d dist",
    "test": "mocha --compilers js:babel-register"
  },
  "author": "Thanh Liem <liemly98@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0"
  }
}
